:- dynamic celda/2.
:- dynamic visitado/1.
:- dynamic turno/1.
:- dynamic celdaAux/2.

:-assert(turno(0)).

comenzar(ColorNuevo):-celda(1,Color), Color\=ColorNuevo, findall(Y,adyacenteC(1,Y),Lista), eliminarRepetidos(Lista,Lista2),
	pintar(Lista2,ColorNuevo), aumentarTurno, retractall(visitado(_)).

aumentarTurno:-turno(T), T2 is T+1, retract(turno(T)), assert(turno(T2)).

chequearVictoria(ColorNuevo):-findall(X,celda(X,_),Celdas), todosiguales(Celdas,ColorNuevo).
todosiguales([],_).
todosiguales([H|T],Color):-celda(H,C), Color=C,todosiguales(T,Color).

adyacenteC(X,Y):-adyacente(X,Y),celda(X,C1),celda(Y,C2), (C1=C2).
adyacenteC(X,Y):-not(visitado(X)),(adyacente(X,Z),celda(X,C1),celda(Z,C2), (C1=C2)),
	not(X=Z),assert(visitado(X)),not(visitado(Z)), adyacenteC(Z,Y).

%Derecha
adyacente(X,Y):- M is(X mod 14),not(M = 0), Indice is X+1,Indice=Y.
%Izquierda
adyacente(X,Y):- M is (X mod 14),not(M=1), Indice is X-1, Indice=Y.
%Arriba
adyacente(X,Y):-((X - 14) > 0), Indice is X-14, Indice=Y.
%Abajo
adyacente(X,Y):- ((X + 14) < 196), Indice is X+14, Indice=Y.

pintar([],ColorNuevo):-pintarAux(1,ColorNuevo).
pintar([H|T],ColorNuevo):-pintarAux(H,ColorNuevo), pintar(T,ColorNuevo).

pintarAux(Indice,ColorNuevo):-celda(Indice,ColorViejo),
	retract(celda(Indice,ColorViejo)), assert(celda(Indice,ColorNuevo)).

cambiarGrilla:-retractall(celda(_,_)), crearGrilla(196),retract(turno(_)),assert(turno(0)).
crearGrilla(1):-Y is random(6),assert(celda(1,Y)).
crearGrilla(X):-Y is random(6),X1 is X-1, assert(celda(X,Y)), crearGrilla(X1).

eliminarRepetidos(ListaIn,ListaOut) :- sort(ListaIn,ListaOut).

ayuda(Color,Cantidad):- celda(1,C), Color=C, Cantidad is 0.
ayuda(Color,Cantidad):- findall(Y,adyacenteC(1,Y),Lista), append([1],Lista,ListaAux), retractall(visitado(_)),
	eliminarRepetidos(ListaAux,ListaOut), length(ListaOut,Size),clonar,pintarClon(ListaOut,Color),
	findall(Y,adyacenteCClon(1,Y),NuevosAdya),append([1],NuevosAdya,NuevosAdyacentes), eliminarRepetidos(NuevosAdyacentes,ListaOut2),
	length(ListaOut2,Size2), Cantidad is (Size2-Size), retractall(visitado(_)),retractall(celdaAux(_,_)).

adyacenteCClon(X,Y):-adyacente(X,Y),celdaAux(X,C1),celdaAux(Y,C2), (C1=C2).
adyacenteCClon(X,Y):-not(visitado(X)),(adyacente(X,Z),celdaAux(X,C1),celdaAux(Z,C2), (C1=C2)),
	not(X=Z),assert(visitado(X)),not(visitado(Z)), adyacenteCClon(Z,Y).


pintarClon([],Color):-pintarAuxClon(1,Color).
pintarClon([H|T],Color):-pintarAuxClon(H,Color), pintarClon(T,Color).

pintarAuxClon(Indice,Color):-celdaAux(Indice,ColorViejo),
 retract(celdaAux(Indice,ColorViejo)), assert(celdaAux(Indice,Color)).

clonar:- findall(X,celda(X,_),Xs),clonar2(Xs).
clonar2([]).
clonar2([H|T]):-celda(H,Y),assert(celdaAux(H,Y)),clonar2(T).

ayuda2(Color,Cant):-ayuda(Color,Cant1),findall(Y,adyacenteC(1,Y),Lista1), eliminarRepetidos(Lista1, Adyacentes),
	celda(1,ColorViejo), pintar(Adyacentes,Color),
	ayuda(0,Rojo),ayuda(1, Violeta), ayuda(2,Rosa), ayuda(3,Verde), ayuda(4,Celeste),ayuda(5,Amarillo),
	comparar([Rojo,Violeta,Rosa,Verde,Celeste,Amarillo], Max),pintar(Adyacentes,ColorViejo) ,Cant is Cant1 + Max.

comparar([H], H).
comparar([H|T], M2) :-  comparar(T, M),  M2 is max(H, M).

